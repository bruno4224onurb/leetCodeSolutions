class Solution:
    def maxArea(self, height: List[int]) -> int:
        left = 0 
        right = len(height) -1
        ammount = 0
        while left < right:
            contender = (min(height[left], height[right]))*(right-left)
            if contender > ammount:
                ammount = contender
            if height[left] > height[right]:
                right -= 1
            else:
                left +=1
        return ammount
        
